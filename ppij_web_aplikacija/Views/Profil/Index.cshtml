                    //seen - skoro isto ko i odustani
                    function seen(id){
                        if($('#hiddenodustani' +id).val() == 'true'){
                            $('#hiddenodustani' +id).val(false)
                        }
                        else{
                            $('#hiddenodustani' +id).val(true)
                        }
